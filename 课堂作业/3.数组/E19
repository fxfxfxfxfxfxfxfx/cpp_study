#include<iostream>
#include<iomanip>
#include<cmath>
#include<string>
#include<vector>
using namespace std;

int main()
{
	int n;
	cin >> n;
	vector <int> number(n);
	for (int i = n - 1; i >= 0; i--)
	{
		cin >> number[i];
	}
	for (int i = 0; i < n; i++)
	{
		cout << number[i] << " ";
	}
	return 0;
}
