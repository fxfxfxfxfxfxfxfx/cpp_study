#include<iostream>
#include<iomanip>
using namespace std;

int main()
{
	int n;
	cin >> n;
	for (int i = 0; i < n; i++)
	{
		cout << setw(8) << setiosflags(ios::right) << (i + 1) * (i + 1);
	}
	cout << endl;
	for (int i = 0; i < n; i++)
	{
		cout << setw(10) << setiosflags(ios::right) << (i + 1) * (i + 1) * (i + 1);
	}
	return 0;
}
